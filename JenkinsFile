pipeline {

  agent any
   
stages {
  stage('Build') {
      steps {
        bat 'mvn package'
      }
    }
       stage('Junit Test Case') {
      steps {
        bat 'mvn test'
      }
    }
    
      stage('SonarQube analysis') {
            steps {
                            
                bat 'mvn  sonar:sonar'
                }
            }
  }
}
   
